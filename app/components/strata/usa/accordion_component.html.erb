<div class="<%= class_names(
  "usa-accordion",
  "usa-accordion--bordered": @is_bordered,
  "usa-accordion--multiselectable": @is_multiselectable
) %>"<%= ' data-allow-multiple' if @is_multiselectable %>>
  <% headings.zip(bodies).each do |heading, body| %>
    <%= heading %>
    <%= body %>
  <% end %>
</div>
